{
  "name": "parsergen-starter",
  "version": "1.0.9",
  "type": "module",
  "description": "A complete parser generator starter with PEG.js, optional Moo lexer, and VS Code integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "parsergen": "./dist/bin/cli.js",
    "lumina": "./dist/bin/lumina.js"
  },
  "scripts": {
    "repl": "tsx src/repl.ts",
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts src/bin/cli.ts src/bin/lumina.ts --format cjs,esm --dts --clean",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch",
    "dev:ui": "vite",
    "build:ui": "vite build",
    "init": "tsx src/bin/cli.ts --init",
    "repl:smoke": "node scripts/repl-smoke.cjs",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:all": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:all:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "prettier": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "keywords": [
    "parser",
    "peg",
    "grammar",
    "lexer",
    "ast",
    "compiler",
    "pegjs",
    "moo"
  ],
  "author": "nyigoro",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jest": "^30.0.0",
    "@types/moo": "^0.5.10",
    "@types/node": "^25.2.2",
    "@types/postcss-import": "^14.0.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.24",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "npm": "^11.9.0",
    "peggy": "^5.0.6",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/language": "^6.11.0",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@swc/core": "^1.15.11",
    "@types/d3-hierarchy": "^3.1.7",
    "@uiw/react-codemirror": "^4.25.4",
    "colorette": "^2.0.20",
    "commander": "^14.0.3",
    "lucide-react": "^0.563.0",
    "moo": "^0.5.2",
    "react-d3-tree": "^3.6.6"
  },
  "engines": {
    "node": ">=22.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nyigoro/parsergen-starter.git"
  },
  "bugs": {
    "url": "https://github.com/nyigoro/parsergen-starter/issues"
  },
  "homepage": "https://github.com/nyigoro/parsergen-starter#readme"
}
