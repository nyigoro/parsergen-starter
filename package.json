{
  "name": "parsergen-starter",
  "version": "1.0.2",
  "type": "module",
  "description": "A complete parser generator starter with PEG.js, optional Moo lexer, and VS Code integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "parsergen": "./dist/cli/index.js"
  },
  "scripts": {
    "repl": "tsx src/repl.ts",
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts src/cli/index.ts --format cjs,esm --dts --clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:ui": "vite",
    "build:ui": "vite build",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "parser",
    "peg",
    "grammar",
    "lexer",
    "ast",
    "compiler",
    "pegjs",
    "moo"
  ],
  "author": "nyigoro",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/moo": "^0.5.10",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.31.0",
    "jest": "^30.0.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-raw": "^1.0.3"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@uiw/react-codemirror": "^4.24.1",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "moo": "^0.5.2",
    "peggy": "^5.0.5"
  },
  "engines": {
    "node": ">=22.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nyigoro/parsergen-starter.git"
  },
  "bugs": {
    "url": "https://github.com/nyigoro/parsergen-starter/issues"
  },
  "homepage": "https://github.com/nyigoro/parsergen-starter#readme"
}
