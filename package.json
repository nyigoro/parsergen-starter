{
  "name": "lumina-lang",
  "version": "0.4.2",
  "type": "module",
  "description": "A modern functional language with async/await, type inference, and package management",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/bin/*-core.*",
    "!dist/bin/*-cjs.*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "RELEASE.md",
    "lumina.config.schema.json",
    "std"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "lumina": "dist/bin/lumina.js",
    "lumina-lsp": "dist/bin/lumina-lsp.js"
  },
  "scripts": {
    "repl": "tsx src/repl.ts",
    "dev": "tsx src/index.ts",
    "build": "npm run build:esm && npm run build:cjs && npm run build:dts && npm run build:clean",
    "build:esm": "tsup src/index.ts src/lumina-runtime.ts src/bin/cli.ts src/bin/lumina.ts src/bin/lumina-lsp.ts src/bin/lumina-worker.ts --format esm --clean",
    "build:cjs": "tsup src/index.ts src/lumina-runtime.ts src/bin/cli-cjs.ts src/bin/lumina-cjs.ts src/bin/lumina-lsp.ts src/bin/lumina-worker.ts --format cjs --clean false && node scripts/rename-cjs.cjs",
    "build:dts": "tsup src/index.ts --format esm --dts --clean false",
    "build:clean": "node scripts/clean-dist-bin.mjs",
    "build:verify": "node scripts/verify-dist.cjs",
    "verify": "npm run lint:check && npm run build && npm test",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch",
    "dev:ui": "vite",
    "build:ui": "vite build && npm run copy:cname && node scripts/build-404.cjs",
    "copy:cname": "node scripts/copy-cname.cjs",
    "init": "tsx src/bin/cli.ts --init",
    "repl:smoke": "node scripts/repl-smoke.cjs",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:all": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:all:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "prettier": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "pack:check": "node scripts/check-package.cjs && npm pack --dry-run"
  },
  "keywords": [
    "lumina",
    "language",
    "compiler",
    "async",
    "functional",
    "typescript"
  ],
  "author": "nyigoro",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jest": "^30.0.0",
    "@types/moo": "^0.5.10",
    "@types/node": "^25.2.2",
    "@types/postcss-import": "^14.0.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.24",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "npm": "^11.9.0",
    "peggy": "^5.0.6",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/language": "^6.11.0",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@swc/core": "^1.15.11",
    "@types/d3-hierarchy": "^3.1.7",
    "@uiw/react-codemirror": "^4.25.4",
    "colorette": "^2.0.20",
    "commander": "^14.0.3",
    "lucide-react": "^0.563.0",
    "moo": "^0.5.2",
    "react-d3-tree": "^3.6.6",
    "fast-glob": "^3.3.3",
    "source-map": "^0.7.4",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11",
    "vscode-uri": "^3.1.0"
  },
  "engines": {
    "node": ">=22.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyigoro/lumina-lang.git"
  },
  "bugs": {
    "url": "https://github.com/nyigoro/lumina-lang/issues"
  },
  "homepage": "https://github.com/nyigoro/lumina-lang#readme"
}
